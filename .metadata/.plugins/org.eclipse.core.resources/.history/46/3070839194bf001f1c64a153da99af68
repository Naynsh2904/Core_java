		} else if(b>a && b>c && b>d) {
			System.out.println(b + " is Greater");
		} else if(c>b && c>b && c>d) {
			System.out.println(c + " is Greater");
		} else if(d>b && d>c && d>a) {
			System.out.println(d + " is Greater");
		} else {
			System.out.println("All no=umber are same");
		}
		
//		find odd number between 229 to 531
		
		int num = 531;
