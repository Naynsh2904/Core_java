package Day_21;

public class Threaaad extends Thread {

	public static void main(String[] args) {
		One o =new One();
		tne t =new tne();
		o.run();
		t.run();
	}

}

class One {
	public void run () {
		for(int i=1 ; i< 5; i++) {
			try {
//				Thread.sleep(500);
				System.out.println("number " + i);
			} catch ( Exception e) {
				System.out.println(e.getMessage());
			}
		}
	}
}

class tne{
	public void run () {
		for(int i=1 ; i< 5; i++) {
			try {
//				Thread.sleep(500);
				System.out.println("number " + i);
			} catch ( Exception e) {
				System.out.println(e.getMessage());
			}
		}
	}
}